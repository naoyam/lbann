model {
  name: "directed_acyclic_graph_model"
  data_layout: "data_parallel"
  mini_batch_size: 256
  block_size: 256
  num_epochs: 72
  num_parallel_readers: 0
  procs_per_trainer: 0

  objective_function {
    layer_term { layer: "cross_entropy" }
    l2_weight_regularization {
      scale_factor: 1e-4
    }
  }

  metric {
    layer_metric {
      name: "categorical accuracy"
      layer: "top1_accuracy"
      unit: "%"
    }
  }
  metric {
    layer_metric {
      name: "top-5 categorical accuracy"
      layer: "top5_accuracy"
      unit: "%"
    }
  }
  
  callback { print {} }
  callback { gpu_memory_usage {} }  
  callback { timer {} }
  callback {
    summary {
      dir: "."
      mat_interval: 0
    }
  }  

  layer {
    name: "data"
    children: "images labels"
    data_layout: "data_parallel"
    parallel_strategy {
      sample_groups: 1
      height_groups: 1
      width_groups: 1
      channel_groups: 1
      filter_groups: 1
    }
    input {
      io_buffer: "partitioned"
    }
  }
  layer {
    name: "images"
    parents: "data"
    data_layout: "data_parallel"
    parallel_strategy {
      sample_groups: 1
      height_groups: 1
      width_groups: 1
      channel_groups: 1
      filter_groups: 1
    }
    split{}
  }
  layer {
    name: "labels"
    parents: "data"
    data_layout: "data_parallel"
