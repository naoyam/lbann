      sample_groups: 1
      height_groups: 1
      width_groups: 1
      channel_groups: 1
      filter_groups: 1
    }
    convolution {
      num_dims: 2
      num_output_channels: 64
      conv_dims_i: 7
      conv_pads_i: 3
      conv_strides_i: 2
      has_bias: false
    }
