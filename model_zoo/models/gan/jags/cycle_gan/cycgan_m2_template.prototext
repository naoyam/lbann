trainer {
}
model {
  data_layout: "data_parallel"
  mini_batch_size: 64
  super_steps: 10000
  num_batches: 1
  num_epochs: 1

  ###################################################
  # Objective function
  ###################################################

  objective_function {
    layer_term {
      scale_factor: 0.05
      layer: "g_adv1_eval"
    }
   # layer_term {
   #   scale_factor: 0.9
   #   layer: "L_cyc_y_eval"
   # }
   # layer_term {
   #   scale_factor: 0.9
   #   layer: "L_cyc_x_eval"
   # }
    layer_term {
      scale_factor: 0.025
      layer: "l_l2_y"
    }
    l2_weight_regularization {
      scale_factor: 1e-4
    }
  }

  ###################################################
  # Metrics
  ###################################################

