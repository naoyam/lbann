model {
  name: "directed_acyclic_graph_model"
  data_layout: "data_parallel"
  mini_batch_size: 64
  block_size: 256
  num_epochs: 18
  num_parallel_readers: 0
  procs_per_model: 0

  ###################################################
  # Objective function
  ###################################################

  objective_function {
    layer_term { layer: "mean_squared_error" }
  }

  ###################################################
  # Metrics
  ###################################################

  # Not yet supported by layer terms.
  #metric { boolean_accuracy {} }
  #metric { boolean_false_positives {} }
  #metric { boolean_false_negatives {} }

  ###################################################
  # Callbacks
  ###################################################
  callback { print {} }
  callback { timer {} }
  #callback { profiler {} }

  ###################################################
  # start of layers
  ###################################################

  layer {
    name: "data"
    children: "images responses"
    data_layout: "data_parallel"
    input {
      io_buffer: "partitioned"
      target_mode: "regression"
    }
  }

  layer {
    name: "images"
    parents: "data"
    data_layout: "data_parallel"
    split {}
  }
  layer {
    name: "responses"
    parents: "data"
    data_layout: "data_parallel"
    split {}
  }

  layer {
    name: "conv1_1"
    parents: "images"
    data_layout: "data_parallel"
    convolution {
      num_dims: 2
      num_output_channels: 128
      conv_dims: "5 5"
      conv_pads: "2 2"
      conv_strides: "2 2"
      has_bias: false
      has_vectors: true
    }
  }
  layer {
    name: "relu1_1"
    parents: "conv1_1"
    data_layout: "data_parallel"
    relu {}
  }
  layer {
    name: "bn1_1"
    parents: "relu1_1"
    data_layout: "data_parallel"
    batch_normalization {
      decay: 0.9
      scale_init: 1.0
      bias_init: 0.0
      epsilon: 1e-5
    }
  }

  layer {
    name: "conv1_2"
    parents: "bn1_1"
    data_layout: "data_parallel"
    convolution {
      num_dims: 2
      num_output_channels: 128
      conv_dims: "3 3"
      conv_pads: "1 1"
      conv_strides: "1 1"
      has_bias: false
      has_vectors: true
    }
  }
  layer {
    name: "relu1_2"
    parents: "conv1_2"
    data_layout: "data_parallel"
    relu {}
  }
  layer {
    name: "bn1_2"
    parents: "relu1_2"
    data_layout: "data_parallel"
    batch_normalization {
      decay: 0.9
      scale_init: 1.0
      bias_init: 0.0
      epsilon: 1e-5
    }
  }

  layer {
    name: "conv1_3"
    parents: "bn1_2"
    data_layout: "data_parallel"
    convolution {
      num_dims: 2
      num_output_channels: 128
      conv_dims: "3 3"
      conv_pads: "1 1"
      conv_strides: "1 1"
      has_bias: false
      has_vectors: true
    }
  }
  layer {
    name: "relu1_3"
    parents: "conv1_3"
    data_layout: "data_parallel"
    relu {}
  }
  layer {
    name: "bn1_3"
    parents: "relu1_3"
    data_layout: "data_parallel"
    batch_normalization {
      decay: 0.9
      scale_init: 1.0
      bias_init: 0.0
      epsilon: 1e-5
    }
  }

  layer {
    name: "conv2_1"
    parents: "bn1_3"
    data_layout: "data_parallel"
    convolution {
      num_dims: 2
      num_output_channels: 256
      conv_dims: "3 3"
      conv_pads: "1 1"
      conv_strides: "2 2"
      has_bias: false
      has_vectors: true
    }
  }
  layer {
    name: "relu2_1"
    parents: "conv2_1"
    data_layout: "data_parallel"
    relu {}
  }
  layer {
    name: "bn2_1"
    parents: "relu2_1"
    data_layout: "data_parallel"
    batch_normalization {
      decay: 0.9
      scale_init: 1.0
      bias_init: 0.0
      epsilon: 1e-5
    }
  }

  layer {
    name: "conv2_2"
    parents: "bn2_1"
    data_layout: "data_parallel"
    convolution {
      num_dims: 2
      num_output_channels: 256
      conv_dims: "3 3"
      conv_pads: "1 1"
      conv_strides: "1 1"
      has_bias: false
      has_vectors: true
    }
  }
  layer {
    name: "relu2_2"
    parents: "conv2_2"
    data_layout: "data_parallel"
    relu {}
  }
  layer {
    name: "bn2_2"
    parents: "relu2_2"
    data_layout: "data_parallel"
    batch_normalization {
      decay: 0.9
      scale_init: 1.0
      bias_init: 0.0
      epsilon: 1e-5
    }
  }

  layer {
    name: "conv2_3"
    parents: "bn2_2"
    data_layout: "data_parallel"
    convolution {
      num_dims: 2
      num_output_channels: 256
      conv_dims: "3 3"
      conv_pads: "1 1"
      conv_strides: "1 1"
      has_bias: false
      has_vectors: true
    }
  }
  layer {
    name: "relu2_3"
    parents: "conv2_3"
    data_layout: "data_parallel"
    relu {}
  }
  layer {
    name: "bn2_3"
    parents: "relu2_3"
    data_layout: "data_parallel"
    batch_normalization {
      decay: 0.9
      scale_init: 1.0
      bias_init: 0.0
      epsilon: 1e-5
    }
  }

  layer {
    name: "conv3_1"
    parents: "bn2_3"
    data_layout: "data_parallel"
    convolution {
      num_dims: 2
      num_output_channels: 256
      conv_dims: "3 3"
      conv_pads: "1 1"
      conv_strides: "2 2"
      has_bias: false
      has_vectors: true
    }
  }
  layer {
    name: "relu3_1"
    parents: "conv3_1"
    data_layout: "data_parallel"
    relu {}
  }
  layer {
    name: "bn3_1"
    parents: "relu3_1"
    data_layout: "data_parallel"
    batch_normalization {
      decay: 0.9
      scale_init: 1.0
      bias_init: 0.0
      epsilon: 1e-5
    }
  }

  layer {
    name: "conv3_2"
    parents: "bn3_1"
    data_layout: "data_parallel"
    convolution {
      num_dims: 2
      num_output_channels: 256
      conv_dims: "3 3"
      conv_pads: "1 1"
      conv_strides: "1 1"
      has_bias: false
      has_vectors: true
    }
  }
  layer {
    name: "relu3_2"
    parents: "conv3_2"
    data_layout: "data_parallel"
    relu {}
  }
  layer {
    name: "bn3_2"
    parents: "relu3_2"
    data_layout: "data_parallel"
    batch_normalization {
      decay: 0.9
      scale_init: 1.0
      bias_init: 0.0
      epsilon: 1e-5
    }
  }

  layer {
    name: "conv3_3"
    parents: "bn3_2"
    data_layout: "data_parallel"
    convolution {
      num_dims: 2
