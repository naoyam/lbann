      epsilon: 1e-5
    }
  }

  layer {
    name: "conv4_2"
    parents: "bn4_1"
    data_layout: "data_parallel"
    convolution {
      num_dims: 2
      num_output_channels: 128
      conv_dims: "3 3"
      conv_pads: "1 1"
      conv_strides: "1 1"
      has_bias: false
      has_vectors: true
    }
  }
  layer {
    name: "relu4_2"
    parents: "conv4_2"
    data_layout: "data_parallel"
    relu {}
  }
  layer {
    name: "bn4_2"
    parents: "relu4_2"
    data_layout: "data_parallel"
    batch_normalization {
      decay: 0.9
      scale_init: 1.0
      bias_init: 0.0
      epsilon: 1e-5
    }
  }

  layer {
    name: "conv4_3"
    parents: "bn4_2"
    data_layout: "data_parallel"
    convolution {
      num_dims: 2
      num_output_channels: 128
      conv_dims: "3 3"
      conv_pads: "1 1"
      conv_strides: "1 1"
      has_bias: false
      has_vectors: true
    }
  }
  layer {
    name: "relu4_3"
    parents: "conv4_3"
    data_layout: "data_parallel"
    relu {}
  }
  layer {
    name: "bn4_3"
    parents: "relu4_3"
    data_layout: "data_parallel"
    batch_normalization {
      decay: 0.9
      scale_init: 1.0
      bias_init: 0.0
      epsilon: 1e-5
    }
  }

  layer {
    name: "fcn16384"
    parents: "bn4_3"
    data_layout: "data_parallel"
    convolution {
      num_dims: 2
      num_output_channels: 256
      conv_dims: "1 1"
      conv_pads: "0 0"
      conv_strides: "1 1"
      has_bias: false
      has_vectors: true
    }
  }

  layer {
    name: "sigmoid"
    parents: "fcn16384"
    data_layout: "data_parallel"
    sigmoid {}
  }

  layer {
    name: "mean_squared_error"
    parents: "sigmoid responses"
    data_layout: "data_parallel"
    mean_squared_error {}
  }

}
