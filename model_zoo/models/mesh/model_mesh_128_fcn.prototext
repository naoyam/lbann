  }
  layer {
    name: "bn3_3"
    parents: "relu3_3"
    data_layout: "data_parallel"
    batch_normalization {
      decay: 0.9
      scale_init: 1.0
      bias_init: 0.0
      epsilon: 1e-5
    }
  }

  layer {
    name: "conv4_1"
    parents: "bn3_3"
    data_layout: "data_parallel"
    convolution {
      num_dims: 2
      num_output_channels: 128
      conv_dims: "3 3"
      conv_pads: "1 1"
      conv_strides: "2 2"
      has_bias: false
      has_vectors: true
    }
  }
  layer {
    name: "relu4_1"
    parents: "conv4_1"
    data_layout: "data_parallel"
    relu {}
